    <property>
      <code>{{velocity}}
## parse the columns to display
#if ("$!xcontext.macro.params.columns" != "")
  #set ($collist = [])
  #foreach ($colname in $xcontext.macro.params.columns.split(','))
    #set ($discard = $collist.add($colname.trim()))
  #end
#else
  #set($collist = ['doc.title','doc.date','doc.author'])
#end
## get whether to display a tagcloud or not
#set($withTagCloud = false)
#if ("$!xcontext.macro.params.tagCloud" == "true") 
  #set($withTagCloud = true)
#end
## get the selected column for the default sort
#set($selectedColumn = "$!xcontext.macro.params.selectedColumn")
## get the selected column for the default sort
#set($defaultOrder = "$!xcontext.macro.params.defaultOrder")
## fill the extra params from the location filter, if any
#set($extraParams = '')
#if ("$!xcontext.macro.params.location" != "") 
  #set ($extraParams = "$!{extraParams}&amp;location=$escapetool.url($xcontext.macro.params.location)")
#end
#set($rowCount = 10)
#if ("$!xcontext.macro.params.rowCount" != '')
  #set($rowCount = $numbertool.toNumber("$!xcontext.macro.params.rowCount"))
#end
## and finally draw the livetable
#set($columnsProperties = {
   "doc.title" : { "type" : "text", "link" : "view", "sortable" : true, "filterable" : true },
   "doc.location" : { 'type': 'text', 'filterable': true, 'html': true},
   "leadingOrganisations" : { "type" : "text" , "html" : true, 'match': "partial"},
   "status" : {"type" : 'list', "html": true},
   "sector" : {"type" : 'list', "html" : true},
   "recognized" : {"type" : 'boolean'},
   "launchDate" : {"type" : 'text', "filterable" : false, "html": true},
   "focalPoint" : {"type" : 'text', "html" : true},
   "leader" : {"type" : 'text', "html" : true},
   "regions" : {"type" : 'list', "html" : true},
   "countries" : {"type" : 'list', "html" : true},
   "description" : {"type" : 'text', "html" : true},
   "website" : {"type" : 'text', "html" : true},
   "progress" : {"type" : 'list', "filterable" : true, "sortable" : true},
   "_actions" : {'actions' : ['edit', 'delete']}
})
#set($options = {
  "className" : "InitiativeCode.InitiativeClass",
  "translationPrefix" : "initiative.livetable.",
  "tagCloud" : $withTagCloud,
  "rowCount": $rowCount,
  "selectedColumn" : $selectedColumn,
  "defaultOrder" : $defaultOrder,
  "extraParams" : $extraParams
})
#livetable("initiatives" $collist $columnsProperties $options)
{{/velocity}}</code>
